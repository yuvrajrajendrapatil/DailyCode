import java.util.*;
class Main {
    public static void main(String[] args) {
        int n ;
        Scanner sc = new Scanner(System.in);
        n = sc.nextInt();
        if(n >= 0){
        System.out.println("+ve");
        }
        else
        {
            System.out.println("-ve");
        }
    }
}
